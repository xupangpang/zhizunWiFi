<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
     android:background="#fff5f5f5"
     android:fitsSystemWindows="true">

    <include
        android:id="@id/viewHeader"
        android:layout_width="fill_parent"
        android:layout_height="48.0dip"
        layout="@layout/view_header1" />

    <include
        android:layout_width="fill_parent"
        android:layout_height="15.0dip"
        layout="@layout/view_split_bottom" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/wifi_setting"
            android:layout_width="fill_parent"
            android:layout_height="50dip"
            android:background="@drawable/btn_list_cell" >

            <ImageView
                android:id="@+id/iv"
                android:layout_width="25.0dip"
                android:layout_height="25.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:background="@drawable/setting_icon_floater" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:layout_toRightOf="@id/iv"
                android:text="WiFi设置"
                android:textColor="#ff333333"
                android:textSize="15dip" />

            <ImageView
                android:layout_width="8dip"
                android:layout_height="10dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:background="@drawable/mine_icon_arrow" />
        </RelativeLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="2.0px"
            android:layout_marginLeft="46.0dip"
            android:background="#fff0f0f0"
            android:contentDescription="@string/empty" />

        <RelativeLayout
            android:id="@+id/message_setting"
            android:layout_width="fill_parent"
            android:layout_height="50dip"
            android:background="@drawable/btn_list_cell" >

            <ImageView
                android:id="@+id/iv2"
                android:layout_width="25.0dip"
                android:layout_height="25.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:background="@drawable/setting_icon_notice" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:layout_toRightOf="@id/iv2"
                android:text="通知设置"
                android:textColor="#ff333333"
                android:textSize="15dip" />

            <ImageView
                android:layout_width="8dip"
                android:layout_height="10dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:background="@drawable/mine_icon_arrow" />
        </RelativeLayout>
    </LinearLayout>

    <include
        android:layout_width="fill_parent"
        android:layout_height="15.0dip"
        layout="@layout/view_split_both" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="50dip"
            android:background="@drawable/btn_list_cell" >

            <ImageView
                android:id="@+id/iv3"
                android:layout_width="25.0dip"
                android:layout_height="25.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:background="@drawable/mine_icon_question" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:layout_toRightOf="@id/iv3"
                android:text="常见问题"
                android:textColor="#ff333333"
                android:textSize="15dip" />

            <ImageView
                android:layout_width="8dip"
                android:layout_height="10dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:background="@drawable/mine_icon_arrow" />
        </RelativeLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="2.0px"
            android:layout_marginLeft="46.0dip"
            android:background="#fff0f0f0"
            android:contentDescription="@string/empty" />

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="50dip"
            android:background="@drawable/btn_list_cell" >

            <ImageView
                android:id="@+id/iv4"
                android:layout_width="25.0dip"
                android:layout_height="25.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:background="@drawable/mine_icon_feedback" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:layout_toRightOf="@id/iv4"
                android:text="意见反馈"
                android:textColor="#ff333333"
                android:textSize="15dip" />

            <ImageView
                android:layout_width="8dip"
                android:layout_height="10dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:background="@drawable/mine_icon_arrow" />
        </RelativeLayout>
    </LinearLayout>

    <include
        android:layout_width="fill_parent"
        android:layout_height="15.0dip"
        layout="@layout/view_split_both" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="50dip"
            android:background="@drawable/btn_list_cell" >

            <ImageView
                android:id="@+id/iv5"
                android:layout_width="25.0dip"
                android:layout_height="25.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:background="@drawable/setting_icon_rate" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:layout_toRightOf="@id/iv5"
                android:text="好评支持一下"
                android:textColor="#ff333333"
                android:textSize="15dip" />

            <ImageView
                android:layout_width="8dip"
                android:layout_height="10dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:background="@drawable/mine_icon_arrow" />
        </RelativeLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="2.0px"
            android:layout_marginLeft="46.0dip"
            android:background="#fff0f0f0"
            android:contentDescription="@string/empty" />

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="50dip"
            android:background="@drawable/btn_list_cell" >

            <ImageView
                android:id="@+id/iv6"
                android:layout_width="25.0dip"
                android:layout_height="25.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:background="@drawable/setting_icon_update" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:layout_toRightOf="@id/iv6"
                android:text="检查更新"
                android:textColor="#ff333333"
                android:textSize="15dip" />

            <ImageView
                android:id="@+id/arrow"
                android:layout_width="8dip"
                android:layout_height="10dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:background="@drawable/mine_icon_arrow" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="5.0dip"
                android:layout_toLeftOf="@id/arrow"
                android:text="当前版本2.2.0"
                android:textColor="#383838"
                android:textSize="12dip" />
        </RelativeLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="2.0px"
            android:layout_marginLeft="46.0dip"
            android:background="#fff0f0f0"
            android:contentDescription="@string/empty" />

        <RelativeLayout
            android:id="@+id/about"
            android:layout_width="fill_parent"
            android:layout_height="50dip"
            android:background="@drawable/btn_list_cell" >

            <ImageView
                android:id="@+id/iv7"
                android:layout_width="25.0dip"
                android:layout_height="25.0dip"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5.0dip"
                android:background="@drawable/setting_icon_floater" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dip"
                android:layout_toRightOf="@id/iv7"
                android:text="关于至尊免费WiFi"
                android:textColor="#ff333333"
                android:textSize="15dip" />

            <ImageView
                android:layout_width="8dip"
                android:layout_height="10dip"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:background="@drawable/mine_icon_arrow" />
        </RelativeLayout>
    </LinearLayout>

    <include
        android:layout_width="fill_parent"
        android:layout_height="15.0dip"
        layout="@layout/view_split_top" />

</LinearLayout>