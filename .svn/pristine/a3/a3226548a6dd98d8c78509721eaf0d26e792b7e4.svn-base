    <declare-styleable name="CheckBox"><attr name="gFont"/><attr name="gBorderSize"/><attr format="dimension" name="gIntervalSize"/><attr format="dimension" name="gMarkSize"/><attr format="color|reference" name="gMarkColor"/></declare-styleable>
