    <dimen name="g_checkBox_intervalSize">2dp</dimen>
    <dimen name="g_checkBox_markSize">22dp</dimen>
    <dimen name="g_editText_hintTitleTextSize">12sp</dimen>
    <dimen name="g_editText_lineSize">1dp</dimen>
    <dimen name="g_editText_lineSize_active_increment">0.5dp</dimen>
    <dimen name="g_editText_paddingH">12dp</dimen>
    <dimen name="g_editText_paddingV">6dp</dimen>
    <dimen name="g_imageView_touch_corners_radius">4dp</dimen>
    <dimen name="g_loading_lineSize">2dp</dimen>
    <dimen name="g_loading_minSize">28dp</dimen>
    <dimen name="g_seekBar_scrubberStroke">4dp</dimen>
    <dimen name="g_seekBar_thumbSize">6dp</dimen>
    <dimen name="g_seekBar_tickSize">0dp</dimen>
    <dimen name="g_seekBar_touchSize">12dp</dimen>
    <dimen name="g_seekBar_trackStroke">2dp</dimen>
