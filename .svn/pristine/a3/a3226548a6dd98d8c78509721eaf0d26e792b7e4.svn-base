    <declare-styleable name="BalloonMarker"><attr name="gFont"/><attr format="reference" name="gMarkerTextAppearance"/><attr format="dimension" name="gMarkerTextPadding"/><attr format="color|reference" name="gMarkerBackgroundColor"/><attr format="dimension" name="gMarkerElevation"/><attr format="dimension" name="gMarkerSeparation"/></declare-styleable>
